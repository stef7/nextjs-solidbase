{
  "name": "nextjs-solidbase",
  "version": "0.0.1",
  "private": true,
  "packageManager": "yarn@3.2.3",
  "scripts": {
    "dev": "yarn cnv && concurrently -k -r 'next dev' 'yarn:watch:*' 'yarn:gen-watch:*' netlify-cms-proxy-server",
    "build": "yarn cnv && concurrently 'yarn:gen:*' 'echo DISABLED yarn:test --ci' && next build",
    "start": "yarn cnv && next start",
    "test": "jest --coverage --colors",
    "lint": "concurrently -n - 'yarn:lint-*'",
    "lint:fix": "concurrently -n - -P 'yarn:lint-* {*}' -- --fix",
    "lint-es": "next lint",
    "lint-ts": "tsc",
    "lint-css": "stylelint 'src/**/*.{css,scss}'",
    "dev-watch:ts": "lint-ts --watch",
    "dev-watch:css": "lint-css",
    "gen:cms": "ts-node -T scripts/generate-cms-types",
    "gen-watch:cms": "tsnd --respawn -T -r tsconfig-paths/register --watch src/cms/config scripts/generate-cms-types",
    "gen:paths": "pathpida",
    "gen-watch:paths": "yarn gen:paths && yarn gen:paths -w",
    "analyse": "cross-env ANALYSE=true next build",
    "cnv": "node scripts/check-node-version",
    "prepare": "yarn cnv && husky install",
    "precommit": "yarn cnv && lint-staged -v"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^12.3.1",
    "@svgr/webpack": "^6.3.1",
    "@tailwindcss/forms": "^0.5.3",
    "clsx": "^1.2.1",
    "fast-deep-equal": "^3.1.3",
    "fs-jetpack": "^5.0.0",
    "gray-matter": "^4.0.3",
    "hash-sum": "^2.0.0",
    "http-status-codes": "^2.2.0",
    "lodash.debounce": "^4.0.8",
    "markdown-to-jsx": "^7.1.7",
    "netlify-cms-app": "^2.15.72",
    "next": "^12.3.0",
    "nprogress": "^0.2.0",
    "probe-image-size": "^7.2.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "sass": "^1.54.9"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/line-clamp": "^0.4.2",
    "@tailwindcss/typography": "^0.5.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/hash-sum": "^1.0.0",
    "@types/lodash.debounce": "^4.0.7",
    "@types/node": "^18.7.18",
    "@types/nprogress": "^0.2.0",
    "@types/probe-image-size": "^7.2.0",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "10.4.10",
    "concurrently": "^7.4.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.23.1",
    "eslint-config-next": "^12.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^43.0.2",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "lint-staged": "^13.0.3",
    "netlify-cms-core": "^2.55.2",
    "netlify-cms-proxy-server": "^1.3.24",
    "netlify-ts": "^1.2.2",
    "pathpida": "^0.18.1",
    "postcss": "8.4.16",
    "prettier": "^2.7.1",
    "stylelint": "^14.11.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "tailwindcss": "3.1.8",
    "ts-essentials": "^9.3.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.3",
    "yaml": "^2.1.1"
  },
  "engines": {
    "node": ">=16.0.0 <17.0.0",
    "npm": "please-use-yarn",
    "yarn": ">=3.2.3"
  }
}
