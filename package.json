{
  "name": "nextjs-solidbase",
  "version": "0.0.1",
  "private": true,
  "packageManager": "yarn@3.2.3",
  "scripts": {
    "dev": "yarn cnv && next dev -p 3005",
    "test": "jest --coverage",
    "build": "yarn cnv && yarn test --ci && next build",
    "start": "yarn cnv && next start",
    "lint-es": "next lint",
    "lint-css": "stylelint 'src/**/*.{css,scss}'",
    "lint": "yarn cnv && concurrently -n - 'yarn:lint-*'",
    "lint:fix": "yarn cnv && concurrently -n - -P 'yarn:lint-* {*}' -- --fix",
    "cnv": "node ./scripts/check-node-version.js",
    "prepare": "husky install",
    "precommit": "lint-staged -v"
  },
  "dependencies": {
    "http-status-codes": "^2.2.0",
    "next": "^12.3.0",
    "nprogress": "^0.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/node": "^18.7.18",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "10.4.10",
    "concurrently": "^7.4.0",
    "eslint": "^8.23.1",
    "eslint-config-next": "^12.3.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^27.0.4",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-unicorn": "^43.0.2",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "lint-staged": "^13.0.3",
    "postcss": "8.4.16",
    "prettier": "^2.7.1",
    "stylelint": "^14.11.0",
    "stylelint-prettier": "^2.0.0",
    "tailwindcss": "3.1.8",
    "typescript": "^4.8.3"
  },
  "engines": {
    "node": ">=16.0.0 <17.0.0",
    "npm": "please-use-yarn",
    "yarn": ">=3.2.3"
  }
}
